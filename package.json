{
  "name": "@zentrick/alpn-agent",
  "version": "0.0.0",
  "description": "Negotiates ALPN and handles transparent https and h2 connections",
  "main": "index.js",
  "author": "Zentrick nv (https://www.zentrick.com/)",
  "contributors": [
    "Pieter Mees <pieter.mees@zentrick.com>"
  ],
  "engines": {
    "node": ">=8"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "lint": "standard",
    "test": "yarn run lint && nyc ava",
    "test:ci": "yarn run lint && nyc ava --tap | tap-xunit >$CIRCLE_TEST_REPORTS/junit.xml"
  },
  "repository": "zentrick/alpn-agent",
  "bugs": "https://github.com/zentrick/alpn-agent/issues",
  "dependencies": {
    "debug": "^3.1.0",
    "lru-cache": "^4.1.3"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "get-port": "^4.0.0",
    "nyc": "^11.7.1",
    "pem": "^1.12.5",
    "standard": "^11.0.1",
    "tap-xunit": "^2.3.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "lib/*.js",
      "index.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js",
      "lib/**/*.js"
    ]
  }
}
